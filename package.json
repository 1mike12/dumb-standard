{
  "name": "dumb-standard",
  "version": "0.0.5",
  "description": "A collection of standard algorithms and data structures for TypeScript projects",
  "type": "commonjs",
  "license": "MS-PL",
  "author": "Mike",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1mike12/dumb-standard.git"
  },
  "homepage": "https://github.com/1mike12/dumb-standard#readme",
  "bugs": {
    "url": "https://github.com/1mike12/dumb-standard/issues"
  },
  "keywords": [
    "typescript",
    "algorithms",
    "data-structures",
    "standard-library"
  ],
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prepublishOnly": "yarn build",
    "build": "yarn barrels && tsup",
    "barrels": "barrelsby --config .barrelsby.json",
    "compile": "tsc",
    "test": "npx mocha",
    "coverage": "nyc --reporter=text-summary --temp-directory=/tmp/nyc_temp npx mocha"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@swc-node/register": "^1.10.9",
    "@swc/core": "^1.3.80",
    "@types/chai": "^5.0.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.11.24",
    "@types/sinon": "^17.0.4",
    "barrelsby": "^2.8.1",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "nyc": "^17.1.0",
    "quick-lru": "^7.0.1",
    "sinon": "^19.0.2",
    "tinybench": "^4.0.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.6",
    "typescript": "^5.0.0"
  }
}